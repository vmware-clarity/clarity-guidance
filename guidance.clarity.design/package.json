{
  "name": "guidance.clarity.design",
  "version": "0.0.0",
  "scripts": {
    "prestart": "npm run prebuild",
    "start": "npm-run-all --parallel --print-label start:*",
    "start:ng-serve": "ng serve",
    "start:compile-pages": "nodemon --watch ../ --ext md --exec \"npm run prebuild:compile-pages\"",
    "prebuild": "npm-run-all prebuild:*",
    "prebuild:compile-pages": "tsx ./scripts/compile-pages.ts",
    "build": "ng build",
    "lint": "npm-run-all lint:*",
    "lint:prettier": "prettier . --check",
    "lint:eslint": "eslint .",
    "lint-fix": "npm-run-all lint-fix:*",
    "lint-fix:prettier": "prettier . --write",
    "lint-fix:eslint": "eslint . --fix",
    "format-md-files": "prettier ../*.md --config ./.prettierrc.js --write"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "18.2.8",
    "@angular/common": "18.2.8",
    "@angular/compiler": "18.2.8",
    "@angular/core": "18.2.8",
    "@angular/forms": "18.2.8",
    "@angular/platform-browser": "18.2.8",
    "@angular/platform-browser-dynamic": "18.2.8",
    "@angular/platform-server": "18.2.8",
    "@angular/router": "18.2.8",
    "@cds/core": "6.13.0",
    "@clr/angular": "17.3.1",
    "@clr/ui": "17.3.1",
    "modern-normalize": "3.0.1",
    "rxjs": "7.8.1",
    "tslib": "2.7.0",
    "zone.js": "0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "18.2.8",
    "@angular/cli": "18.2.8",
    "@angular/compiler-cli": "18.2.8",
    "@types/prismjs": "1.26.4",
    "angular-eslint": "18.3.1",
    "domino": "2.1.6",
    "eslint": "9.12.0",
    "eslint-plugin-import": "2.31.0",
    "front-matter": "4.0.2",
    "marked": "14.1.2",
    "marked-gfm-heading-id": "4.1.0",
    "marked-highlight": "2.1.4",
    "nodemon": "3.1.7",
    "npm-run-all": "4.1.5",
    "prettier": "3.3.3",
    "prismjs": "1.29.0",
    "tsx": "4.19.1",
    "typescript": "5.5.4",
    "typescript-eslint": "8.8.1"
  }
}
